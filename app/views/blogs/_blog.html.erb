<div id="<%= dom_id blog %>">
  <p>
    <strong>First name:</strong>
    <%= blog.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= blog.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= blog.email %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= blog.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= blog.content %>
  </p>

</div>
